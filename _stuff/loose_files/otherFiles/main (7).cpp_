
#include "main.h"
#include "gpio.h"
#include "usart.h"

void Error_Handler(uint32_t errorCode);
void SystemClock_Config(void);
void setupGpio(void);

int main(void)
{
	HAL_Init();
	SystemClock_Config();
	setupGpio();
	MX_USART1_UART_Init();

	char text[100];
	text[0] = 'A';

	while(1) {
		HAL_UART_Transmit(&huart1, (uint8_t *)text, 1, 100);
		HAL_Delay(1000);
	}
}
